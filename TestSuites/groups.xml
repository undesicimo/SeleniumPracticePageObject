<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite parallel ="tests" name="Suite">

	<groups>
		<run>
			<include name="ErrorHandling"/>
		</run>
	</groups>
	<listeners>
		<listener class-name="frameworkPractice.testComponents.Listeners"/>
	</listeners>
  <test thread-count="5" name="Order number check">
    <classes>
      <class name="frameworkPractice.test.StandaloneTest2"/>
    </classes>
  </test> <!-- Test -->
  <test thread-count="5" name="Log in Error check">
  	<classes>
  		<class name="frameworkPractice.test.ErrorCheck1"/>
  	</classes>
   </test>
</suite> <!-- Suite -->
